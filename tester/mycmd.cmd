if %menu%==Y goto Yes
if %menu%==y goto Yes
if %menu%==N goto No
if %menu%==n goto No
cls
echo.
echo Mmm try again...
echo.
set /p pause="Press any key to continue!... "
goto A

:Yes
cls
taskkill /F /IM "vrchat.exe" /T
title stopping vrchat.exe
killall VRChat.exe
